<div class = "formlabel">
<h1>Login</h1>

<%= form_tag(authenticate_path) do %>
  
  <div class="field">
    <%= label_tag :netid %><br>
    <%= text_field_tag :username %>
  </div>
  <div class="field">
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
    <!--default passwords are "password" -->
  </div>
  <div class="actions">
    <%= submit_tag "Login" %>
  </div>
<% end %>
</div>